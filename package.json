{
  "name": "league-prod-toolkit-core",
  "version": "1.7.2",
  "scripts": {
    "build": "tsc && webpack",
    "build:modules": "node dist/utils/build-modules.js",
    "update": "node dist/utils/update.js",
    "postbuild": "copyfiles core/web/views/**.* core/web/public/**.* dist",
    "start": "node --trace-warnings dist/core/app.js",
    "tsc": "tsc",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix",
    "prettier": "prettier --write --ignore-unknown ./**",
    "setuphooks": "git config --local core.hooksPath .hooks",
    "hooks:pre-commit": "npm run lint && npm run build"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "bootstrap": "^4.5.0",
    "cli-progress": "^3.12.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "express-fileupload": "^1.3.1",
    "express-rate-limit": "^6.9.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.6",
    "jquery": "^3.7.0",
    "jsonwebtoken": "^9.0.2",
    "jspath": "^0.4.0",
    "jwt-decode": "^3.1.2",
    "minimist": "^1.2.8",
    "nanospinner": "^1.1.0",
    "pug": "^3.0.0",
    "semver": "^7.5.4",
    "send": "^0.18.0",
    "toastr": "^2.1.4",
    "tslib": "^2.6.2",
    "uniqid": "^5.3.0",
    "uuid-apikey": "^1.5.3",
    "validator": "^13.11.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0",
    "zip-lib": "^0.7.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.2.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/minimist": "^1.2.0",
    "@types/node": "^20.5.7",
    "@types/send": "^0.17.1",
    "@types/uniqid": "^5.3.0",
    "@types/validator": "^13.11.1",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.2.0",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "nodemonConfig": {
    "watch": [
      "core",
      "modules"
    ],
    "exec": "npm start",
    "ext": "ts,js,json,pug,css"
  }
}
